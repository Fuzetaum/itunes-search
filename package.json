{
  "name": "itunes-search",
  "version": "0.1",
  "description": "iTunes search portal",
  "author": "Ricardo Fuzeto",
  "engines": {
    "node": ">= 10.16.0"
  },
  "private": true,
  "scripts": {
    "build": "webpack --config webpack/prod.config.js",
    "serve": "webpack --config webpack/serve.config.js",
    "start": "webpack-dev-server --config webpack/dev.config.js --color --hot",
    "build:analyze": "webpack --config webpack/prod.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.12.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-polyfill": "6.26.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "3.2.0",
    "eslint": "6.5.1",
    "eslint-plugin-react": "7.16.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "moment-locales-webpack-plugin": "1.1.0",
    "node-sass": "7.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "regenerator-runtime": "0.13.3",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "stylelint": "11.0.0",
    "url-loader": "2.2.0",
    "webpack": "4.41.0",
    "webpack-bundle-analyzer": "3.5.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "4.2.2",
    "webpack-plugin-serve": "0.12.1",
    "webpack-serve": "3.2.0"
  },
  "dependencies": {
    "antd": "3.25.0",
    "axios": "0.19.0",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router-dom": "5.1.2"
  }
